		(function () {
			function a() {
				window._sf_endpt = (new Date()).getTime();

				var cbDomain = (("https:"==document.location.protocol)?"https://a248.e.akamai.net/chartbeat.download.akamai.com/102508/":"http://static.chartbeat.com/");

